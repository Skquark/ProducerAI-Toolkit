{
  "name": "producer-ai-scraper",
  "version": "1.0.0",
  "description": "Automated scraper for Producer.ai music library - downloads songs, metadata, lyrics, and cover art",
  "main": "src/main.js",
  "type": "module",
  "bin": {
    "producer-scraper": "./cli.js"
  },
  "scripts": {
    "start": "node cli.js",
    "scrape": "node cli.js download --all",
    "scrape:playlist": "node cli.js playlist",
    "test": "node scripts/test.js",
    "setup": "node scripts/setup.js",
    "login": "node cli.js login",
    "download": "node cli.js download",
    "export": "node cli.js export",
    "status": "node cli.js status",
    "cli": "node cli.js"
  },
  "keywords": [
    "producer.ai",
    "riffusion",
    "music",
    "backup",
    "download",
    "music-backup",
    "scraper",
    "automation",
    "playwright",
    "browser-automation"
  ],
  "author": "Skquark",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Skquark/ProducerAI-Toolkit.git"
  },
  "homepage": "https://github.com/Skquark/ProducerAI-Toolkit#readme",
  "bugs": {
    "url": "https://github.com/Skquark/ProducerAI-Toolkit/issues"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^14.0.2",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "json2csv": "^6.0.0-alpha.2",
    "node-id3": "^0.2.9",
    "ora": "^8.1.1",
    "p-queue": "^8.0.1",
    "playwright": "^1.48.0",
    "sanitize-filename": "^1.6.3",
    "winston": "^3.15.0",
    "yargs": "^17.7.2"
